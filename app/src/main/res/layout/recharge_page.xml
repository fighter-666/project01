<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="360dp"
    android:layout_height="wrap_content"
    android:background="#f4f4f4">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.example.myapplication.widget.TitleBar
            android:id="@+id/tvTv1"
            android:layout_width="match_parent"
            android:layout_height="39dp"
            android:gravity="center"
            android:textColor="#333333"
            app:back_src="@drawable/back"
            app:imageHeight3="18dp"
            app:imageWidth3="12dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:right_title="评价" />
    </androidx.appcompat.widget.Toolbar>


    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/toolbar">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="13dp"
            android:paddingRight="13dp">

            <!-- 在这里添加你的约束布局内容 -->
            <ImageView
                android:id="@+id/ivIv1"
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:layout_marginTop="23dp"
                android:src="@drawable/tick"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@id/tvTv2"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tvTv2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="成功充值 100元"
                android:textColor="#333333"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintLeft_toRightOf="@id/ivIv1"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tvTv3"
                android:layout_width="189dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="27dp"
                android:gravity="left"
                android:text="充值号码"
                android:textColor="#999999"
                android:textSize="14sp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tvTv2" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="173 666 99999"
                android:textColor="#333333"
                android:textSize="14sp"
                app:layout_constraintBaseline_toBaselineOf="@id/tvTv3"
                app:layout_constraintTop_toBottomOf="@id/tvTv2" />

            <TextView
                android:id="@+id/tvTv4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:layout_marginRight="175dp"
                android:gravity="left"
                android:text="支付金额"
                android:textColor="#999999"
                android:textSize="14sp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tvTv3" />

            <TextView
                android:id="@+id/tvTv5"
                android:layout_width="52dp"
                android:layout_height="15dp"
                android:layout_marginTop="14dp"
                android:layout_marginRight="6dp"
                android:background="@drawable/shape_rectangle17"
                android:gravity="center"
                android:text="官方-0.5元"
                android:textColor="#fe5561"
                android:textSize="9sp"
                app:layout_constraintRight_toLeftOf="@id/tvTv6"
                app:layout_constraintTop_toBottomOf="@id/tvTv3" />

            <TextView
                android:id="@+id/tvTv6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:gravity="right"
                android:text="99.5元"
                android:textColor="#333333"
                android:textSize="14sp"
                app:layout_constraintBaseline_toBaselineOf="@id/tvTv4"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tvTv3" />

            <TextView
                android:id="@+id/tv21tv"
                android:layout_width="0dp"
                android:layout_height="36dp"
                android:layout_marginTop="27dp"
                android:background="@drawable/shape_rectangle"
                android:gravity="center"
                android:text="继续充值"
                android:textColor="#333333"
                android:textSize="16sp"
                app:layout_constraintHorizontal_chainStyle="spread"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@id/tv2Tv"
                app:layout_constraintTop_toBottomOf="@id/tvTv6" />

            <com.example.myapplication.recharge.widget.BalanceInquiryView
                android:id="@+id/tv2Tv"
                android:layout_width="0dp"
                android:layout_height="36dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="27dp"
                android:background="@drawable/shape_rectangle2"
                app:imageHeight="17dp"
                app:imageHeight2="14dp"
                app:imageWidth="16dp"
                app:imageWidth2="16dp"
                app:layout_constraintLeft_toRightOf="@id/tv21tv"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tvTv6"
                app:textColor="@android:color/white"
                app:textContent="查询余额" />


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl1"
                android:layout_width="match_parent"
                android:layout_height="38dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/shape_rectangle3"
                app:layout_constraintTop_toBottomOf="@id/tv2Tv"
                tools:ignore="MissingConstraints">

                <com.example.myapplication.recharge.widget.ScrollImageView
                    android:id="@+id/im1Im"
                    android:layout_width="23dp"
                    android:layout_height="18dp"
                    android:layout_marginLeft="10dp"
                    android:src="@drawable/beans"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


                <com.example.myapplication.recharge.widget.ScrollTextView
                    android:id="@+id/tv2Tv2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="10dp"
                    android:text="充值奖励 "
                    android:textColor="#666666"
                    android:textSize="13sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toRightOf="@id/im1Im"
                    app:layout_constraintRight_toLeftOf="@id/tv2Tv3"
                    app:layout_constraintTop_toTopOf="parent" />

                <com.example.myapplication.recharge.widget.ScrrollTextViewBackground
                    android:id="@+id/tv2Tv3"
                    android:layout_width="52dp"
                    android:layout_height="22dp"
                    android:layout_marginRight="10dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toLeftOf="@id/tv2Image"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/tv2Image"
                    android:layout_width="10dp"
                    android:layout_height="10dp"
                    android:layout_marginRight="13dp"
                    android:src="@drawable/close"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:background="@drawable/shape_rectangle5"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/cl1"
                tools:ignore="MissingConstraints">

                <TextView
                    android:id="@+id/cl2Tv1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="15dp"
                    android:text="为您推荐以下服务"
                    android:textColor="#333333"
                    android:textSize="16sp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="17dp"
                    android:layout_marginRight="6dp"
                    android:text="更多"
                    android:textColor="#999999"
                    android:textSize="12sp"
                    app:layout_constraintRight_toLeftOf="@id/cl2Im1"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/cl2Im1"
                    android:layout_width="5dp"
                    android:layout_height="9dp"
                    android:layout_marginRight="12dp"
                    android:src="@drawable/arrow"
                    app:layout_constraintBottom_toBottomOf="@id/cl2Tv1"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="@id/cl2Tv1" />

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginBottom="15dp"
                    app:layout_constraintBottom_toBottomOf="@id/cl2"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/cl2Tv1"
                    app:layout_constraintVertical_bias="0.0" />


            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl2Copy"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:background="@drawable/shape_rectangle5"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/cl2"
                tools:ignore="MissingConstraints">

                <TextView
                    android:id="@+id/cl2Tv1Copy"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="15dp"
                    android:text="为您推荐以下服务"
                    android:textColor="#333333"
                    android:textSize="16sp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="17dp"
                    android:layout_marginRight="6dp"
                    android:text="更多"
                    android:textColor="#999999"
                    android:textSize="12sp"
                    app:layout_constraintRight_toLeftOf="@id/cl2Im1Copy"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/cl2Im1Copy"
                    android:layout_width="5dp"
                    android:layout_height="9dp"
                    android:layout_marginRight="12dp"
                    android:src="@drawable/arrow"
                    app:layout_constraintBottom_toBottomOf="@id/cl2Tv1Copy"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="@id/cl2Tv1Copy" />

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerViewCopy"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginBottom="15dp"
                    app:layout_constraintBottom_toBottomOf="@id/cl2Copy"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/cl2Tv1Copy"
                    app:layout_constraintVertical_bias="0.0" />


            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl3"
                android:layout_width="match_parent"
                android:layout_height="269dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/shape_rectangle5"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/cl2Copy"
                tools:ignore="MissingConstraints">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cl3Cl1"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="16dp"
                    android:background="@drawable/ellipse"
                    android:gravity="center"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <ImageView
                        android:id="@+id/cl3_cl1Im"
                        android:layout_width="21dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="9dp"
                        android:src="@drawable/image5"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView
                    android:id="@+id/cl3Tv1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="6dp"
                    android:layout_marginTop="16dp"
                    android:text="开启自动充值得金豆"
                    android:textColor="#333333"
                    android:textSize="14sp"
                    app:layout_constraintLeft_toRightOf="@id/cl3Cl1"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/cl3Tv2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="首次开启得"
                    android:textColor="#999999"
                    android:textSize="12sp"
                    app:layout_constraintBottom_toBottomOf="@id/cl3Cl1"
                    app:layout_constraintLeft_toLeftOf="@id/cl3Tv1" />

                <ImageView
                    android:id="@+id/cl3Tv3"
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:layout_marginLeft="4dp"
                    android:src="@drawable/bean"
                    app:layout_constraintBottom_toBottomOf="@id/cl3Cl1"
                    app:layout_constraintLeft_toRightOf="@id/cl3Tv2"
                    app:layout_constraintTop_toTopOf="@id/cl3Tv2" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="10金豆"
                    android:textColor="#f3bb62"
                    android:textSize="12sp"
                    app:layout_constraintBaseline_toBaselineOf="@id/cl3Tv2"
                    app:layout_constraintLeft_toRightOf="@id/cl3Tv3" />

                <TextView
                    android:layout_width="50dp"
                    android:layout_height="24dp"
                    android:layout_marginLeft="220dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/shape_rectangle9"
                    android:gravity="center"
                    android:text="去开启"
                    android:textColor="#5b82ff"
                    android:textSize="13sp"
                    app:layout_constraintLeft_toRightOf="@id/cl3Cl1"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="70dp"
                    android:layout_marginRight="12dp"
                    android:background="@drawable/shape_rectangle10"
                    android:color="#eeeeee"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cl3Cl2"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="86dp"
                    android:background="@drawable/ellipse"
                    android:gravity="center"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <ImageView
                        android:id="@+id/cl3Cl2Im"
                        android:layout_width="21dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="9dp"
                        android:src="@drawable/image6"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView
                    android:id="@+id/cl3Tv4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="6dp"
                    android:text="看科普视频得金豆"
                    android:textColor="#333333"
                    android:textSize="14sp"
                    app:layout_constraintLeft_toRightOf="@id/cl3Cl1"
                    app:layout_constraintTop_toTopOf="@id/cl3Cl2" />

                <TextView
                    android:id="@+id/cl3Tv5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="多看多得浏览15s得多看多得..."
                    android:textColor="#999999"
                    android:textSize="12sp"
                    app:layout_constraintBottom_toBottomOf="@id/cl3Cl2"
                    app:layout_constraintLeft_toLeftOf="@id/cl3Tv4" />

                <ImageView
                    android:id="@+id/cl3Tv6"
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:layout_marginLeft="4dp"
                    android:layout_marginTop="2dp"
                    android:src="@drawable/bean"
                    app:layout_constraintBottom_toBottomOf="@id/cl3Tv5"
                    app:layout_constraintLeft_toRightOf="@id/cl3Tv5"
                    app:layout_constraintTop_toTopOf="@id/cl3Tv5" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="5金豆"
                    android:textColor="#f3bb62"
                    android:textSize="12sp"
                    app:layout_constraintBaseline_toBaselineOf="@id/cl3Tv5"
                    app:layout_constraintLeft_toRightOf="@id/cl3Tv6" />

                <TextView
                    android:layout_width="50dp"
                    android:layout_height="24dp"
                    android:layout_marginLeft="220dp"
                    android:layout_marginTop="94dp"
                    android:background="@drawable/shape_rectangle9"
                    android:gravity="center"
                    android:text="去看看"
                    android:textColor="#5b82ff"
                    android:textSize="13sp"
                    app:layout_constraintLeft_toRightOf="@id/cl3Cl2"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/tv2"
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="140dp"
                    android:layout_marginRight="12dp"
                    android:background="@drawable/shape_rectangle10"
                    android:color="#eeeeee"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/cl3Tv7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="13dp"
                    android:layout_marginTop="156dp"
                    android:gravity="left"
                    android:text="推荐兑换好物："
                    android:textColor="#333333"
                    android:textSize="14sp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/cl3Tv8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="4dp"
                    android:text="共有"
                    android:textColor="#999999"
                    android:textSize="13sp"
                    app:layout_constraintBottom_toBottomOf="@id/cl3Tv7"
                    app:layout_constraintRight_toLeftOf="@id/cl3Tv9"
                    app:layout_constraintTop_toTopOf="@id/cl3Tv7" />

                <ImageView
                    android:id="@+id/cl3Tv9"
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:src="@drawable/bean"
                    app:layout_constraintBottom_toBottomOf="@id/cl3Tv7"
                    app:layout_constraintRight_toLeftOf="@id/cl3Tv11"
                    app:layout_constraintTop_toTopOf="@id/cl3Tv7" />

                <TextView
                    android:id="@+id/cl3Tv11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="14dp"
                    android:text="1356金豆"
                    android:textColor="#f3bb62"
                    android:textSize="12sp"
                    app:layout_constraintBottom_toBottomOf="@id/cl3Tv7"
                    app:layout_constraintRight_toLeftOf="@id/cl3Tv12"
                    app:layout_constraintTop_toTopOf="@id/cl3Tv7" />


                <TextView
                    android:id="@+id/cl3Tv12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="4dp"
                    android:text="逛更多"
                    android:textColor="#999999"
                    android:textSize="12sp"
                    app:layout_constraintBottom_toBottomOf="@id/cl3Tv7"
                    app:layout_constraintRight_toLeftOf="@id/clIma1"
                    app:layout_constraintTop_toTopOf="@id/cl3Tv7" />

                <ImageView
                    android:id="@+id/clIma1"
                    android:layout_width="5dp"
                    android:layout_height="9dp"
                    android:layout_marginRight="12dp"
                    android:src="@drawable/arrow"
                    app:layout_constraintBottom_toBottomOf="@id/cl3Tv12"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="@id/cl3Tv12" />

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerView2"
                    android:layout_width="match_parent"
                    android:layout_height="75dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="15dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/cl3Tv7" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <com.example.myapplication.recharge.widget.FlipCardView
                android:id="@+id/cl4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="13dp"
                android:background="@drawable/shape_rectangle5"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/cl3"
                tools:ignore="MissingConstraints" />

            <com.example.myapplication.recharge.widget.ScratchCardView
                android:id="@+id/cl6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:background="@drawable/shape_rectangle5"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/cl4" />


            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerView4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/cl6" />

            <View
                android:id="@+id/view"
                android:layout_width="match_parent"
                android:layout_height="79dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/recyclerView4" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>